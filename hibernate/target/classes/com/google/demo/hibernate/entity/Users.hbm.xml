<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.google.demo.hibernate.entity.Users" table="users" catalog="bookshop">     
     <id name="id" type="java.lang.Integer">
         <column name="Id" />
         <generator class="native"></generator>
     </id>
     <property name="loginName" type="java.lang.String">
         <column name="LoginName" length="50" />
     </property>
     <property name="loginPwd" type="java.lang.String">
         <column name="LoginPwd" length="16" />
     </property>
     <property name="name" type="java.lang.String">
         <column name="Name" length="16" />
     </property>
     <property name="address" type="java.lang.String">
         <column name="Address" length="16" />
     </property>
     <property name="phone" type="java.lang.String">
         <column name="Phone" length="16" />
     </property>
     <property name="mail" type="java.lang.String">
         <column name="Mail" length="16" />
     </property>     
 </class>
</hibernate-mapping>
